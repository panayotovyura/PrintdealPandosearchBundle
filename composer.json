{
    "name": "printdeal/pandosearch-bundle",
    "description": "Symfony PandosearchBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Olexy Romanchenko",
            "email": "o.romanchenko@levi9.com"
        },
        {
            "name": "Yuriy Panayotov",
            "email": "i.panayotov@levi9.com"
        }
