    "require-dev": {
        "phpunit/phpunit": "^6.1|^7.0",
        "symfony/phpunit-bridge": "^4.0",
        "squizlabs/php_codesniffer": "^3.0",
        "sebastian/phpcpd": "^3.0|^4.0",
        "phpmd/phpmd" : "^2.6",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^4.0",
        "phpstan/phpstan": "^0.9"
    },
    "config": {
        "bin-dir": "bin"
    }
